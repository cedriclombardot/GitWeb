{% set active="code" %}
{% use "GitWebRepositoryBundle:Default:_common/tabs.html.twig" %}
{% use "GitWebRepositoryBundle:Default:_common/infos.html.twig" %}
{% use "GitWebRepositoryBundle:Default:_common/tabs_code.html.twig" %}
{% use "GitWebRepositoryBundle:Default:_common/breadcrumb.html.twig" %}

{% block body %}
	{{ block('repo_head') }}
	{{ block('repo_tabs') }}
	{{ block('repo_infos') }}
	
